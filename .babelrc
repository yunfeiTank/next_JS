{
    "presets": [
        "next/babel",
        [
            "@babel/env",
            {
                "targets": {
                    "edge": "17",
                    "firefox": "60",
                    "chrome": "67",
                    "safari": "11.1"
                }
            }
        ]
    ],
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "lib",
                "style": "index.css"
            }
        ],
        [
            "import",
            {
                "libraryName": "@ant-design/icons",
                "libraryDirectory": "lib/icons",
                "camel2DashComponentName": false
            },
            "@ant-design/icons"
        ]
    ]
}